  "dependencies": {
    "body-parser": "^1.20.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "http-status-codes": "^2.2.0",
    "marked": "^4.1.0",
    "mongoose": "^6.6.1",
    "multer": "^1.4.5-lts.1",
